@model IEnumerable<Calories.Models.Food>
@{
    ViewBag.Title = "Foods";
    Layout = "/Views/Shared/_Layout.cshtml";

}

<form asp-action="Eat" asp-controller="Home">

    <div>
        @foreach (Food f in Model)
        {
            <button name="ID" value="@f.ID">@f.Name</button>
        }
    </div>

</form>
